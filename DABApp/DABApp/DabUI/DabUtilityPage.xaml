<?xml version="1.0" encoding="UTF-8"?>
<local:DabBaseContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="DABApp.DabUtilityPage" xmlns:local="clr-namespace:DABApp">
    <ContentPage.Content>
        <StackLayout VerticalOptions="FillAndExpand">
            <Label Text="Welcome to the DAB Utility Page. You can do some interesting things here!"/>
            <Label Text="Imitate GraphQL Response"/>
            <Picker x:Name="pickAction"/>
            <Button Text="Imitate GraphQL Response" x:Name="btnGraphQlResponse" Clicked="btnGraphQlResponse_Clicked"/>
            <Label Text="Show Table Data"/>
            <Picker x:Name="pickTable"/>
            <Button Text="Show Table Data" x:Name="btnShowTableData" Clicked="btnShowTableData_Clicked"/>
            <Button VerticalOptions="End" Text="Close" x:Name="btnClose" Clicked="btnClose_Clicked"/>
        </StackLayout>

    </ContentPage.Content>
</local:DabBaseContentPage>
