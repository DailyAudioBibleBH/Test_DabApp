<?xml version="1.0" encoding="UTF-8"?>
<local:DabBaseContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="DABApp.DabChannelsPage" 
		xmlns:t="clr-namespace:SlideOverKit;assembly=SlideOverKit" xmlns:local="clr-namespace:DABApp;" BackgroundColor="Black">
	<t:MenuContainerPage.Content>
		<StackLayout>
			<Label Text="Channels" TextColor="White"/>
			<Button Text="Episodes" TextColor="White" Clicked="OnEpisodes"/>
			<Button Text="Player" Clicked="OnPlayer"/>
			<Button Text="Test" Clicked="OnTest"/>
			<Button Text="Browse" Clicked="OnBrowse"/>
			<ContentView VerticalOptions="EndAndExpand"/>
		</StackLayout>
	</t:MenuContainerPage.Content>
</local:DabBaseContentPage>
