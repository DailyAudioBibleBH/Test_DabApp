<?xml version="1.0" encoding="UTF-8"?>
<local:DabBaseContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="DABApp.DabSignUpPage"
	xmlns:local="clr-namespace:DABApp;">
	<ContentPage.Content>
		<StackLayout Padding="10" Spacing="10">
			<local:HtmlLabel Text="{Binding signUp}"/>
			<Label Text="First Name" FontSize="Micro"/>
			<Entry x:Name="FirstName"/>
			<Label Text="Last Name" FontSize="Micro"/>
			<Entry x:Name="LastName"/>
			<Label Text="Email Address" FontSize="Micro"/>
			<Entry x:Name="Email"/>
			<Label Text="Password" FontSize="Micro"/>
			<Entry x:Name="Password"/>
			<Button Text="Sign up" Style="{StaticResource highlightedButtonStyle}" Clicked="OnSignUp"/>
		</StackLayout>
	</ContentPage.Content>
</local:DabBaseContentPage>
