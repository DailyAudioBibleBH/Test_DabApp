<?xml version="1.0" encoding="UTF-8"?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="DABApp.DabRootPage"
		xmlns:local="clr-namespace:DABApp;assembly=DABApp" BackgroundColor="Black">
	<MasterDetailPage.Master>
		<local:DabMenuPage x:Name="dabMenuPage"/>
	</MasterDetailPage.Master>
	<MasterDetailPage.Detail>
		<NavigationPage BarTextColor="White" BarBackgroundColor="Black">
			<x:Arguments>
				<local:DabChannelsPage />
			</x:Arguments>
		</NavigationPage>
	</MasterDetailPage.Detail>
</MasterDetailPage>
