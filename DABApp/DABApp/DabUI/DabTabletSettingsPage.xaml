<?xml version="1.0" encoding="UTF-8"?>
<local:MasterDetailMenuPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="DABApp.DabTabletSettingsPage"
	xmlns:local="clr-namespace:DABApp;">
	<NavigationPage.ToolbarItems>
		<ToolbarItem x:Name="menuButton" Priority="1" Icon="ic_menu_white.png" Clicked="OnMenu"/>
		<ToolbarItem x:Name="giveButton" Priority="0" Text="Give"/>
	</NavigationPage.ToolbarItems>
	<MasterDetailPage.Master>
		<local:DabSettingsPage x:Name="SettingsPage"/>
	</MasterDetailPage.Master>
	<MasterDetailPage.Detail>
		<local:DabAppInfoPage x:Name="AppInfoPage"/>
	</MasterDetailPage.Detail>
</local:MasterDetailMenuPage>
